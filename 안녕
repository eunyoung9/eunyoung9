public class PrimeNumber {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

		int num = 1;
		
		for(int i = 0; i < 100; i++) {
			if(Prime_number(num)==true)
				System.out.println(num + "소수입니다.");
			else
				System.out.println(num + "소수가 아님 ");
			
			num ++;
		}
		
	}
